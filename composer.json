{
    "name": "crmplease/coder",
    "license": "MIT",
    "type": "library",
    "description": "Coder is library for code-generation, e.g. add property to class, parameter to method, value to array and etc.",
    "require": {
        "rector/rector": "^0.7.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Crmplease\\Coder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Crmplease\\Coder\\": "tests"
        }
    }
}
