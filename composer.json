{
    "name": "crmplease/coder",
    "license": "MIT",
    "type": "library",
    "description": "Coder is library for code-generation, e.g. add property to class, parameter to method, value to array and etc.",
    "keywords": ["coder", "code-generation"],
    "homepage": "https://github.com/crmplease/coder",
    "require": {
        "rector/rector": "^0.8.56",
        "nikic/php-parser": "^4.10",
        "phpstan/phpdoc-parser": "^0.4.9",
        "symfony/dependency-injection": "^5.2",
        "symplify/set-config-resolver": "^8.3",
        "symplify/smart-file-system": "^8.3",
        "symplify/package-builder": "^8.3",
        "symfony/console": "^5.2"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^9.4.3"
    },
    "autoload": {
        "psr-4": {
            "Crmplease\\Coder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Crmplease\\Coder\\": "tests"
        }
    }
}
